{
  "name": "yunzhutong-web-cloudflare",
  "version": "1.0.0",
  "description": "云协通Web主控端 - Cloudflare Workers部署",
  "main": "src/worker.js",
  "scripts": {
    "build": "flutter build web --release",
    "build:silent": "./build-silent.sh",
    "deploy": "./deploy-pages.sh",
    "deploy:silent": "./deploy-silent.sh",
    "deploy:quick": "./deploy-silent.sh",
    "dev": "wrangler dev",
    "preview": "wrangler preview",
    "tail": "wrangler tail",
    "clean": "rm -rf build/web",
    "setup": "npm install -g wrangler && wrangler login",
    "logs": "wrangler pages deployment tail --project-name yunzhutong-web",
    "status": "wrangler pages deployment list --project-name yunzhutong-web"
  },
  "keywords": [
    "flutter",
    "web",
    "cloudflare",
    "workers",
    "meeting",
    "webrtc"
  ],
  "author": "云协通团队",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.60.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/meeting_pro.git"
  }
} 